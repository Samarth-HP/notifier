{
  "cSpell.words": ["nestjs"],
  "editor.formatOnSave": true,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "files.associations": {
    "*.env": "env"
  },
  "files.exclude": {
    "**/bin": true,
    "**/obj": true,
    "pdata": true,
    "broker": true,
    "tilt_modules": true,
    "**/node_modules/": true
  }
}
